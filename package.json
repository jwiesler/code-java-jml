{
  "name": "java-jml",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.2.0",
  "publisher": "Julian Wiesler",
  "license": "MIT",
  "engines": {
    "vscode": "*"
  },
  "contributes": {
    "languages": [
      {
        "id": "java",
        "extensions": [
          ".java",
          ".jav"
        ],
        "aliases": [
          "Java-JML",
          "java-jml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "java",
        "scopeName": "source.java",
        "path": "./syntaxes/java-jml.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "java",
        "path": "./snippets/java-jml.code-snippets"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jwiesler/code-java-jml.git"
  }
}
